{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "producer": {
        "bootstrapservers": "localhost:50012"
    },
    "consumer": {
        "bootstrapservers": "localhost:50012", //specify your kafka broker address
        "groupid": "csharp-consumer",
        "enableautocommit": true,
        "statisticsintervalms": 5000,
        "sessiontimeoutms": 6000,
        "autooffsetreset": 0,
        "enablepartitioneof": true
    }
}
